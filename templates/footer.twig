<footer id="footer" class="container">
	<nav id="access" class="main-navigation navbar" role="navigation">
	  <div class="container-fluid">
		  <ul class="nav nav-justified">
			  <li class="menu-item"><a href="{{site.url}}" rel="home">{{site.name}}</a></li>
		  {% for item in menu.items %}
			  <li id="menu-item-{{item.ID}}" class="menu-item menu-item-type-{{item.type}} menu-item-object-{{item.object}} menu-item-{{item.ID}}">
				  <a href="{{item.link}}">{{item.title}}</a>
			  </li>
		  {% endfor %}
			  <li class="menu-item"><a>Copyright {{"now"|date('Y')}}</a></li>
		  </ul>
	  </div>
	</nav>
</footer>

<script src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
<script src="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>